<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{% block title %}{% endblock %} | Benoit Menier</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<header class="sticky-top">
    {{ include('base/_header.html.twig') }}
</header>
<main class="">

    {% block body %}{% endblock %}
</main>
<!-- FOOTER -->
<footer class="py-8 bg-dark mt-5">

   {{ include('base/_footer.html.twig') }}
</footer>
</body>
</html>
